<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>ClientRegistration</servlet-name>
        <servlet-class>ClientRegistration</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ClientLogin</servlet-name>
        <servlet-class>ClientLogin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Control</servlet-name>
        <servlet-class>Control</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServerOTPCheck</servlet-name>
        <servlet-class>ServerOTPCheck</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UserInvalidate</servlet-name>
        <servlet-class>UserInvalidate</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>FileUpload</servlet-name>
        <servlet-class>FileUpload</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>FileDetails</servlet-name>
        <servlet-class>FileDetails</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AccessCheck</servlet-name>
        <servlet-class>AccessCheck</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GetOTP</servlet-name>
        <servlet-class>GetOTP</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ClientRegistration</servlet-name>
        <url-pattern>/ClientRegistration</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ClientLogin</servlet-name>
        <url-pattern>/ClientLogin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Control</servlet-name>
        <url-pattern>/Control</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServerOTPCheck</servlet-name>
        <url-pattern>/ServerOTPCheck</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UserInvalidate</servlet-name>
        <url-pattern>/UserInvalidate</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>FileUpload</servlet-name>
        <url-pattern>/FileUpload</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>FileDetails</servlet-name>
        <url-pattern>/FileDetails</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AccessCheck</servlet-name>
        <url-pattern>/AccessCheck</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GetOTP</servlet-name>
        <url-pattern>/GetOTP</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
